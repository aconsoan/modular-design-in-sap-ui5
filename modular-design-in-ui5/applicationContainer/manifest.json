{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "com.haojia.test.app",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"embeds": ["student", "exam"]
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"componentUsages": {
			"student": {
				"name": "com.haojia.test.student",
				"componentData": {
					"routePatternPrefix": "student&/"
				}
			},
			"exam": {
				"name": "com.haojia.test.exam",
				"componentData": {
					"routePatternPrefix": "exam&/"
				}
			}
		},
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {}
			},
			"components": {
				"com.haojia.test.student": {},
				"com.haojia.test.exam": {}
			}
		},
		"componentData": {
	    	"routePatternPrefix": "modularDemo/"
	    },
		"rootView": "com.haojia.test.applicationContainer.view.App",
		"routing": {
			"config": {
				"routerClass": "com.haojia.test.control.ModularRouter",
				"viewType": "XML",
				"viewPath": "com.haojia.test.applicationContainer.view",
				"transition": "show",
				"controlId": "adminShell",
				"clearTarget": true,
				"controlAggregation": "pages"
			},
			"routes": [{
				"pattern": "",
				"name": "Main",
				"target": "Main"
			}, {
				"pattern": "{module}&:all*:",
				"name": "AppShell",
				"target": "AppShell"
			}],
			"targets": {
				"Main": {
					"viewName": "Main",
					"viewLevel": 0
				},
				"AppShell": {
					"viewName": "AppShell",
					"viewLevel": 0
				}
			}
		}
	}
}