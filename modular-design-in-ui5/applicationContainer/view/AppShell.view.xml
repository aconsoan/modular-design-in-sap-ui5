<mvc:View controllerName="com.haojia.test.applicationContainer.controller.AppShell" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	xmlns:hj="com.haojia.test.control" displayBlock="true">
	<Page >
		<headerContent>
			<OverflowToolbar class="moduleHeader">
				<ToolbarSpacer/>
				<Button text="Go to Student Module" type="Transparent" press="studentMain">
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</Button>
				<Button text="Go to Student Module -> List View" type="Transparent" press="studentList">
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</Button>
				<Button text="Go to Exam Module -> List View" type="Transparent" press="examList">
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</Button>
			</OverflowToolbar>
		</headerContent>
		<content>
			<hj:ComponentManager id="applicationComponentLoader"></hj:ComponentManager>
		</content>
	</Page>
</mvc:View>